{
  "settings": {
    "index.percolator.map_unmapped_fields_as_string": true,
    "index.mapping.total_fields.limit": 3000
  },
  "mappings": {
    "file_v0.0.4": {
      "dynamic": "strict",
      "numeric_detection": true,
      "_meta": {
        "_owner": "CernSearch-Administrators@cern.ch"
      },
      "properties": {
        "_access": {
          "type": "object",
          "properties": {
            "owner": {
              "type": "keyword"
            },
            "read": {
              "type": "keyword"
            },
            "update": {
              "type": "keyword"
            },
            "delete": {
              "type": "keyword"
            }
          }
        },
        "_data": {
          "type": "object",
          "properties": {
            "title": {
              "type": "keyword"
            },
            "_attachment": {
              "dynamic": true,
              "type": "object",
              "properties": {
                "_content": {
                  "type": "text",
                  "fields": {
                    "english": {
                      "type": "text",
                      "analyzer": "english"
                    },
                    "french": {
                      "type": "text",
                      "analyzer": "french"
                    }
                  }
                }
              }
            }
          }
        },
        "_bucket": {
          "type": "keyword"
        },
        "_bucket_content": {
          "type": "keyword"
        },
        "_file": {
          "type": "keyword"
        },
        "_url": {
          "type": "keyword"
        },
        "_updated": {
          "type": "date"
        },
        "_created": {
          "type": "date"
        },
        "control_number": {
          "type": "keyword"
        },
        "$schema": {
          "enabled": false
        }
      }
    }
  }
}
